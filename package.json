{
  "name": "vue3-bottom-sheet",
  "version": "0.1.12",
  "private": false,
  "scripts": {
    "dev": "vite --config vite-demo.config.ts serve --host 0.0.0.0",
    "build": "rimraf dist &&  vite build",
    "build-demo": "rimraf dist &&  vite build --config vite-demo.config.ts",
    "deploy": "rimraf dist && vite build && npm publish --access=public",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.11",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue": "^3.4.27",
    "vue-tsc": "^1.8.6",
    "vue3-colorpicker": "^2.1.6"
  },
  "peerDependencies": {
    "vue": "^3.4.27"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "main": "dist/vue-bottom-sheet.umd.js",
  "module": "dist/vue-bottom-sheet.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-bottom-sheet.mjs",
      "require": "./dist/vue-bottom-sheet.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./mjs": "./dist/vue-bottom-sheet.mjs",
    "./components/*": "./components/*.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrytran9704/vue-bottom-sheet.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "component",
    "bottom-sheet"
  ],
  "author": "henry-tran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henrytran9704/vue-bottom-sheet/issues"
  },
  "homepage": "https://github.com/henrytran9704/vue-bottom-sheet#readme"
}
